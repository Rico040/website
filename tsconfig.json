{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "types": [
      "vite-plugin-solid-svg/types-component-solid",
      "vinxi/client",
      "vite/client"
    ],
    "isolatedModules": true,
    "skipLibCheck": true,
    "paths": {
      "~/*": ["./src/*"]
    }
  }
}
